{
    "name": "ydyachenko/db-queries-toolbar",
    "type": "library",
    "description": "Db toolbar for ZendDeveloperTools",
    "keywords": [
        "debug",
        "developer",
        "zf",
        "module",
        "db"
    ],
    "homepage": "https://github.com/YDyachenko/DbQueriesToolbar",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy Dyachenko"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-db": ">=2.1.0",
        "zendframework/zend-developer-tools": "^1.1.0",
        "zendframework/zend-mvc": "^2.7 || ^3.0.1"
    },
    "require-dev": {
        "zendframework/zend-coding-standard": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "DbQueriesToolbar": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
